<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="de.spitak.amazinggame.activity.MenuActivity"
    >

    <Button
        android:id="@+id/gameTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClickGameStart"
        android:text="@string/button_game_test_start"
        />

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClickShowHighscore"
        android:text="@string/button_show_highscore" />

</LinearLayout>
